@model userDashboardASP.Models.ValidateViewModel

@{
    ViewData["Title"] = "Home Page";
}
<h1>Register</h1>
<form action="Register" method="POST">
<!-- ======================================================================================== -->
<!-- ======================================================================================== -->
    <label for="firstName">First Name:</label>
    <input asp-for="firstName" type="text" name="firstName">
    <span asp-validation-for="firstName"></span>
<!-- ======================================================================================== -->
<!-- ======================================================================================== -->

    <label for="lastName">Last Name:</label>
    <input asp-for="lastName" type="text" name="lastName">
    <span asp-validation-for="lastName"></span>
<!-- ======================================================================================== -->
<!-- ======================================================================================== -->

    <label for="email">Email:</label>
    <input asp-for="email" type="text" name="email">
    <span asp-validation-for="email"></span>
<!-- ======================================================================================== -->
<!-- ======================================================================================== -->

    <label for="password">Password:</label>
    <input asp-for="password" type="password" name="password">
    <span asp-validation-for="password"></span>
<!-- ======================================================================================== -->
<!-- ======================================================================================== -->

    <label for="confirmPassword">Confirm Password:</label>
    <input asp-for="confirmPassword" type="password" name="confirmPassword"><br>
    <span asp-validation-for="confirmPassword"></span>
<!-- ======================================================================================== -->
<!-- ======================================================================================== -->

    <input type="submit" name="submit">
    
<!-- ======================================================================================== -->
<!-- ======================================================================================== -->
    
</form>

<a href="Login"><button>Or Click Here To Login</button></a>